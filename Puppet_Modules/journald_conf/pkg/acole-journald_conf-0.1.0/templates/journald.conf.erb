[Journal]
<% if @storage -%>
Storage=<%= @storage %>
<% else -%>
#Storage=auto
<% end -%>
<% if @compress -%>
Compress=<%= @compress %>
<% else -%>
#Compress=yes
<% end -%>
<% if @seal -%>
Seal=<%= @seal %>
<% else -%>
#Seal=yes
<% end -%>
<% if @splitmode -%>
SplitMode=<%= @splitmode %>
<% else -%>
#SplitMode=uid
<% end -%>
<% if @syncintervalsec -%>
SyncIntervalSec=<%= @syncintervalsec %>
<% else -%>
#SyncIntervalSec=5m
<% end -%>
<% if @ratelimitinterval -%>
RateLimitInterval=<%= @ratelimitinterval %>
<% else -%>
#RateLimitInterval=30s
<% end -%>
<% if @ratelimitburst -%>
RateLimitBurst=<%= @ratelimitburst %>
<% else -%>
#RateLimitBurst=1000
<% end -%>
<% if @systemmaxuse -%>
SystemMaxUse=<%= @systemmaxuse %>
<% else -%>
#SystemMaxUse=
<% end -%>
<% if @systemkeepfree -%>
SystemKeepFree=<%= @systemkeepfree %>
<% else -%>
#SystemKeepFree=
<% end -%>
<% if @systemmaxfilesize -%>
SystemMaxFileSize=<%= @systemmaxfilesize %>
<% else -%>
#SystemMaxFileSize=
<% end -%>
<% if @runtimemaxuse -%>
RuntimeMaxUse=<%= @runtimemaxuse %>
<% else -%>
#RuntimeMaxUse=
<% end -%>
<% if @runtimekeepfree -%>
RuntimeKeepFree=<%= @runtimekeepfree %>
<% else -%>
#RuntimeKeepFree=
<% end -%>
<% if @runtimemaxfilesize -%>
RuntimeMaxFileSize=<%= @runtimemaxfilesize %>
<% else -%>
#RuntimeMaxFileSize=
<% end -%>
<% if @maxretentionsec -%>
MaxRetentionSec=<%= @maxretentionsec %>
<% else -%>
#MaxRetentionSec=
<% end -%>
<% if @maxfilesec -%>
MaxFileSec=<%= @maxfilesec %>
<% else -%>
#MaxFileSec=1month
<% end -%>
<% if @forwardtosyslog -%>
ForwardToSyslog=<%= @forwardtosyslog %>
<% else -%>
#ForwardToSyslog=yes
<% end -%>
<% if @forwardtokmsg -%>
ForwardToKMsg=<%= @forwardtokmsg %>
<% else -%>
#ForwardToKMsg=no
<% end -%>
<% if @forwardtoconsole -%>
ForwardToConsole=<%= @forwardtoconsole %>
<% else -%>
#ForwardToConsole=no
<% end -%>
<% if @forwardtoconsole -%>
ForwardToWall=<%= @forwardtoconsole %>
<% else -%>
#ForwardToWall=yes
<% end -%>
<% if @ttypath -%>
TTYPath=<%= @ttypath %>
<% else -%>
#TTYPath=/dev/console
<% end -%>
<% if @maxlevelstore -%>
MaxLevelStore=<%= @maxlevelstore %>
<% else -%>
#MaxLevelStore=debug
<% end -%>
<% if @maxlevelsyslog -%>
MaxLevelSyslog=<%= @maxlevelsyslog %>
<% else -%>
#MaxLevelSyslog=debug
<% end -%>
<% if @maxlevelkmsg -%>
MaxLevelKMsg=<%= @maxlevelkmsg %>
<% else -%>
#MaxLevelKMsg=notice
<% end -%>
<% if @maxlevelconsole -%>
MaxLevelConsole=<%= @maxlevelconsole %>
<% else -%>
#MaxLevelConsole=info
<% end -%>
<% if @maxlevelwall -%>
MaxLevelWall=<%= @maxlevelwall %>
<% else -%>
#MaxLevelWall=emerg
<% end -%>
<% if @linemax -%>
LineMax=<%= @linemax %>
<% else -%>
#LineMax=48K
<% end -%>










